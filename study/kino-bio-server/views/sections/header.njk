<header class='header'>
  <div class='navigation__mobile'>
    <a href='/kino-bio-projekt/index'>
      <div class='header__logo'></div>
    </a>
    <button class='hamburger' aria-label='Menu'>
      <span class='bar'></span>
      <span class='bar'></span>
      <span class='bar'></span>
    </button>
  </div>
  <ul class='hamburger__items'>
    {% for item in menuItems %}
      <li class='hamburger__list'>
        <a href='{{ item.link }}'>{{ item.label }}</a>
      </li>
    {% endfor %}
  </ul>
  <a href='/'>
    <div class='header__logo'></div>
  </a>
  <nav class='header__nav'>
    <ul class='header__nav-list'>
      {% for item in menuItems %}
        <li class='header__nav-item'>
          <a href='{{ item.link }}' class='header__nav-link' aria-label='Go to Biljetter'>{{ item.label }}</a>
        </li>
      {% endfor %}
    </ul>
  </nav>
  <a class='header__bookTicket' aria-label='Book tickets now'>
    <button class='header__BookTicket-button'>BOKA BILJETT</button>
  </a>
</header>